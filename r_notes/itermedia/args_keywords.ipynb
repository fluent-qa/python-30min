   "source": [
    "def func_args(*args,**kwargs):\n",
    "  print(args)\n",
    "  print(type(args))\n",
    "  print(kwargs)\n",
    "  print(type(kwargs))\n",
    "  for item in args:\n",
    "    print(item)\n",
    "  for item in kwargs:\n",
    "    print(item,kwargs[item])\n",
    "\n",
    "func_args(1,2,3,4,5,a=123,b=456)\n"
   ],
